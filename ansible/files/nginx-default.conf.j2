server {
        listen       80 default_server;
        server_name  {{ inventory_hostname }};
        root /var/www/html/ ;
 
	client_max_body_size 64M;
 
        location / {
                index index.html index.htm;
        }
 
        location ~* \.(gif|jpg|jpeg|png|css|js)$ {
                expires max;
        }
 
}
